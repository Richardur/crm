<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="100dp"
    android:height="100dp"
    android:viewportWidth="100"
    android:viewportHeight="100">

    <!-- Wider Lambda Part with rounded edges -->
    <path
        android:strokeWidth="10"
        android:strokeColor="#000000"

        android:strokeLineCap="round"
        android:pathData="M 20,90 L 50,20 L 80,90" />

    <!-- Vertical line of 'i' with rounded edges -->
    <path
        android:strokeWidth="12"
        android:strokeColor="#000000"

        android:strokeLineCap="round"
        android:pathData="M 50,87.7 L 50,75" />

    <!-- Dot of 'i' as a path centered -->
    <path
        android:strokeWidth="6"
        android:strokeColor="#000000"
        android:fillColor="#000000"
        android:pathData="M 50,55 a 5,5 0 1,1 0,10 a 5,5 0 1,1 0,-10" />

    <!-- Inverted and smaller lambda resembling a 'v' -->
    <path
        android:strokeWidth="8"
        android:strokeColor="#000000"

        android:strokeLineCap="round"
        android:pathData="M 30,70 L 50,90 L 70,70" />

</vector>
